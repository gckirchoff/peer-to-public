		"@sveltejs/kit": "^2.8.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/d3": "^7.4.3",
		"@typescript-eslint/eslint-plugin": "^8.14.0",
		"@typescript-eslint/parser": "^8.14.0",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.8",
		"sass": "^1.80.7",
		"svelte": "^5.1.16",
		"svelte-check": "^4.0.7",
		"tslib": "^2.8.1",
